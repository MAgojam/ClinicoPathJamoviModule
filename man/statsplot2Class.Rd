  dep = "mpg",
  group = "cyl",
  direction = "independent",
  distribution = "p"
)

# Repeated measures design with alluvial diagram
statsplot2(
  data = survey_data,
  dep = "condition_baseline", 
  group = "condition_followup",
  direction = "repeated",
  alluvsty = "t1"
)
}

